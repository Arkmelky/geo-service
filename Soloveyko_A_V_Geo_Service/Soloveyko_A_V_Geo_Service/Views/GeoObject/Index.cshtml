@model IEnumerable<Soloveyko_A_V_Geo_Service.Models.GeoObject>

@{
    ViewBag.Title = "Index";
}

<h2>Список объектов</h2>

<p>
    @Html.ActionLink("Добавить новый объект", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address.Country)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GeoObjectType.GeoObjectTypeName)
        </th>
        <th style="    width: 170px;"></th>
    </tr>

@foreach (var item in Model) {
    <tr >
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address.Country)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GeoObjectType.GeoObjectTypeName)
        </td>
        <td>
            @Html.ActionLink("Править", "Edit", new { id=item.GeoObjectId }) 
            @Html.ActionLink("Просмотреть", "Details", new { id=item.GeoObjectId }) 
            @Html.ActionLink("Удалить", "Delete", new { id=item.GeoObjectId })
        </td>
    </tr>
    <tr style="height:15px;"></tr>
}

</table>
